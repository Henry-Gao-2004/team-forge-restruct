generator client {
  provider = "prisma-client-js"
}
 
datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Entry{
  id String  @id @default(uuid())
  password String 
  name String
  email String @unique
  createdAt DateTime @default(now())
  skills Skills?
  title String?
  content String?
}

enum Skills{
  Java
  Python
  C
  

}